<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Agoda.Analyzers</id>
    <version>$Version$</version>
    <title>Agoda Roslyn Analyzers</title>
    <authors>Agoda</authors>
    <owners>Agoda</owners>
    <projectUrl>https://github.com/agoda-com/AgodaAnalyzers</projectUrl>
    <iconUrl>https://avatars3.githubusercontent.com/u/19235448?s=200</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Agoda Roslyn Analyzers</description>
    <tags>Agoda,Agoda Roslyn Analyzers, analyzers, Roslyn</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <!-- The convention for analyzers is to put language agnostic dlls in analyzers\portable50 and language specific analyzers in either analyzers\portable50\cs or analyzers\portable50\vb -->
  <files>
    <file src="*.dll" target="analyzers\dotnet\cs" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*;**\System.Composition.*;**\Newtonsoft.Json*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>